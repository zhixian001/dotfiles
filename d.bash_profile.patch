# For MacOS
#export GPG_TTY=$(tty)

# Detect OS / Processor
current_kernel_name="$(uname -s)"

if [[ $current_kernel_name == "Darwin" ]]; then
    # Mac OS
    current_cpu_m1="$(sysctl -a | grep machdep.cpu.brand_string | tr " " "\n" |  grep M1)"
    if [[ $current_cpu_m1 == "M1" ]]; then
		for f in $HOME/dotfiles/apple_silicon/*.sh; do
        	source "$f"
        done
    fi
fi
