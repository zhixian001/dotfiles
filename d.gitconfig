[user]
    name       = <TBU>
    email      = <TBU>
#    signingkey = <TBU>

[core]
    autocrlf   = false
    editor     = code

[merge]
    tool       = vscode

[mergetool "vscode"]
    cmd = code --wait --merge $REMOTE $LOCAL $BASE $MERGED

[diff]
    tool       = vscode

[difftool "vscode"]
    cmd        = code --wait --diff $LOCAL $REMOTE

